{
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "has": [
        {
          "type": "host",
          "value": "www.sunred.vip"
        }
      ],
      "destination": "https://sunred.vip",
      "permanent": true
    }
  ]
}